  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development",
      "env": {
        "EXPO_ENV": "development" // EXPO_ENV 환경 변수 설정
      }
    },
    "staging": {
      "channel": "staging",
      "env": {
        "EXPO_ENV": "staging" // 스테이징 빌드 시 EXPO_ENV=staging으로 설정
      },
      "buildType": "apk" // Android, iOS는 "archive"
    },
    "production": {
      "channel": "production",
      "env": {
        "EXPO_ENV": "production" // 운영 빌드 시 EXPO_ENV=production으로 설정
      },
      "buildType": "apk" // Android, iOS는 "archive"
    }
  }
}
